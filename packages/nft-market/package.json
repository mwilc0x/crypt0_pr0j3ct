{
  "name": "@crypto-org/nft-market",
  "version": "0.69.0",
  "scripts": {
    "client-dev": "yarn workspace @crypto-org/nft-market-client dev",
    "client-prod": "yarn workspace @crypto-org/nft-market-client prod",
    "server-dev": "NODE_ENV=development yarn workspace @crypto-org/nft-market-server start",
    "server-prod": "NODE_ENV=production yarn workspace @crypto-org/nft-market-server start",
    "image-api-dev": "NODE_ENV=development yarn workspace @crypto-org/nft-market-image-api start",
    "image-api-prod": "NODE_ENV=production yarn workspace @crypto-org/nft-market-image-api start",
    "prod": "yarn client-prod && yarn server-prod",
    "dev": "concurrently \"yarn client-dev\" \"yarn server-dev\" \"yarn image-api-dev\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
