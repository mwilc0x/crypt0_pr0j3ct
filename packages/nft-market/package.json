{
  "name": "@crypto-org/nft-market",
  "version": "0.69.0",
  "scripts": {
    "client-dev": "NODE_ENV=development yarn workspace @crypto-org/nft-market-client dev",
    "client-prod": "NODE_ENV=production yarn workspace @crypto-org/nft-market-client prod",
    "fe-server-dev": "NODE_ENV=development yarn workspace @crypto-org/fe-server start",
    "fe-server-prod": "NODE_ENV=production yarn workspace @crypto-org/fe-server start",
    "img-api-dev": "NODE_ENV=development yarn workspace @crypto-org/img-api-server start",
    "img-api-prod": "NODE_ENV=production yarn workspace @crypto-org/img-api-server start",
    "prod": "yarn client-prod && yarn server-prod",
    "dev": "concurrently \"yarn client-dev\" \"yarn fe-server-dev\" \"yarn img-api-dev\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
